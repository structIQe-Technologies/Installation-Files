[
    {
        "id": "cad_assist",
        "name": "CAD Assist",
        "repo": "https://github.com/structIQe-Technologies/Setup-Files-CAD-Assist",
        "Folder": "CAD Assist",
        "version": "0.0.0",
        "install_path": "C:\\Program Files (x86)\\structIQe Launcher\\CAD Assist",
        "Modules": [
            "Plugin",
            "Support Files"
        ],
        "release_notes": "Initial release of CAD Assist.",
        "icon": "cadassist_icon.png",
        "install_steps": [
            {
                "action": "copy",
                "from": "C:\\Program Files (x86)\\structIQe Launcher\\CAD Assist\\Library\\Lib",
                "to": [
                    "C:\\_Temp\\NEW",
                    "C:\\Program Files\\Autodesk\\ApplicationPlugins"
                ]
            },
            {
                "action": "copy",
                "from": "C:\\Program Files (x86)\\structIQe Launcher\\CAD Assist\\Library\\Other APIs",
                "to": [
                    "C:\\Program Files\\Autodesk\\AutoCAD 2020",
                    "C:\\Program Files\\Autodesk\\AutoCAD 2021",
                    "C:\\Program Files\\Autodesk\\AutoCAD 2022",
                    "C:\\Program Files\\Autodesk\\AutoCAD 2023"
                ]
            }
        ],
        "UninstallDisplayNames": []
    },





    
